<table class="table table-striped table-bordered">
  <thead>
  <tr>
    <th>Student</th>
    <th>Review</th>
    <th>Rating</th>
    <th>Action</th>
  </tr>
  </thead>

  <tbody>
  <% reviews.each do |review| %>
    <tr>
      <td><%= review.lesson.student.name %></td>
      <td><%= review.review %></td>
      <td><%= review.rating %></td>
      <td>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>

